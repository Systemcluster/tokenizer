{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "dynamicImport": true,
            "decorators": true,
            "dts": false
        },
        "transform": {
            "optimizer": {}
        },
        "target": "es2020",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true,
        "preserveAllComments": false
    },
    "sourceMaps": true,
    "minify": false
}
